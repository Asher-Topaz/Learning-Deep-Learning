    "def compute_output(w, x):\n",
    "    z = 0.0\n",
    "    for i in range(len(w)):\n",
    "        z += x[i] * w[i] #compute sum of weighted inputs\n",
    "    if z < 0:\n",
    "        return -1\n",
    "    else:\n",
    "        return 1\n"
